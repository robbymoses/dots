{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#1e1e0c90",
                "editor.background": "#1e1e0c90",
                "editor.foreground": "#c6c6c2",
                "text": "#c6c6c2",
                "text.muted": "#c6c6c270",
                "text.ignored": "#c6c6c240",
                "text.placeholder": "#c6c6c250",
                "ignored": "#c6c6c230",
                "element.hover": "#c6c6c230",
                "ghost_element.hover": "#3D445A30",
                "ghost_element.selected": "#3D445A30",
                "ghost_element.active": "#3D445A60",
                "border": "#1e1e0c",
                "editor.highlighted_line.background": "#3D445A10",
                "editor.active_line.background": "#3D445A10",
                "panel.background": "#1e1e0c90",
                "title_bar.background": "#1e1e0c90",
                "title_bar.inactive_background": "#1e1e0c90",
                "status_bar.background": "#1e1e0c90",
                "drop_target.background": "#1e1e0c90",
                "elevated_surface.background": "#1e1e0c",
                "toolbar.background": "#1e1e0c90",
                "tab_bar.background": "#1e1e0c90",
                "tab.inactive_background": "#1e1e0c90",
                "tab.active_background": "#3D445A30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#1e1e0c",
                "scrollbar.thumb.background": "#3D445A",
                "editor.gutter.background": "#1e1e0c90",
                "terminal.background": "#1e1e0c10",
                "terminal.foreground": "#c6c6c2",
                "terminal.dim_foreground": "#c6c6c2",
                "terminal.bright_foreground": "#c6c6c2",
                "terminal.ansi.black": "#1e1e0c",
                "terminal.ansi.red": "#3D445A",
                "terminal.ansi.green": "#484C71",
                "terminal.ansi.yellow": "#553E86",
                "terminal.ansi.blue": "#5E569B",
                "terminal.ansi.magenta": "#686EB1",
                "terminal.ansi.cyan": "#766ACB",
                "terminal.ansi.white": "#c6c6c2",
                "terminal.ansi.bright_black": "#70705c",
                "terminal.ansi.bright_red": "#3D445A",
                "terminal.ansi.bright_green": "#484C71",
                "terminal.ansi.bright_yellow": "#553E86",
                "terminal.ansi.bright_blue": "#5E569B",
                "terminal.ansi.bright_magenta": "#686EB1",
                "terminal.ansi.bright_cyan": "#766ACB",
                "terminal.ansi.bright_white": "#c6c6c2",
                "modified": "#3D445A",
                "syntax": {
                    "attribute": {
                        "color": "#3D445A"
                    },
                    "boolean": {
                      "color": "#484C71"
                    },
                    "comment": {
                      "color": "#70705c"
                    },
                    "comment.doc": {
                      "color": "#70705c"
                    },
                    "constant": {
                      "color": "#484C71"
                    },
                    "function": {
                      "color": "#766ACB"
                    },
                    "keyword": {
                      "color": "#5E569B"
                    },
                    "number": {
                      "color": "#686EB1"
                    },
                    "operator": {
                      "color": "#5E569B"
                    },
                    "string": {
                      "color": "#3D445A"
                    },
                    "variable": {
                      "color": "#484C71"
                    }
                },
                "players": [
                  {
                    "cursor": "#3D445A",
                    "background": "#1e1e0c",
                    "selection": "#3D445A30"
                  }
                ]
            }
        }
    ]
}
